[user]
	name = ansoncat
	email = ansoncat@gmail.com
[color]
	ui = auto
	status = auto
	diff = auto
	branch = auto
[core]
	editor = vim
[alias]
	bn = !sh -c 'git branch | sed -n \"/* /s///p\"'
	t = tfs
	tp = git tfs pull -I
	tc = git tfs checkintool -I
	ai = add --interactive
	co = checkout
	cow = checkout work
	com = checkout master
	c = commit
	cm = commit -m
	cma = commit -a -m
	ca = commit --amend
	caa = commit -a --amend -C HEAD
	st = stash
	sp = stash pop
	rb = rebase
	rbm = rebase master
	rbw = rebase work
	reh = reset HEAD
	me = merge
	mem = merge master
	mew = merge work
	r = reset
	d = diff
	dc = diff --cached
	s = status
	b = branch
	re = remote
	f = fetch
	pl = pull
	p = push
	l = log
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
	cpk = cherry-pick
	cpkis = cherry-pick -X ignore-space-change -X ignore-all-space -X ignore-space-at-eol
	cfg = config -l
[merge]
	tool = vimdiff
[push]
	default = matching
[gui]
        encoding = utf-8
